# ArchLens Strategic Systems - Environment Configuration
# Copy this file to .env and fill in your actual values

# IMPORTANT: Variables prefixed with VITE_ are exposed to the browser
# Never put sensitive server-only secrets with VITE_ prefix

# Gemini AI API Key (Required for AI-powered audits)
# Note: This will be exposed to the browser - use with caution in production
VITE_GEMINI_API_KEY=your_gemini_api_key_here
VITE_API_KEY=your_gemini_api_key_here
VITE_PAYPAL_CLIENT_ID=your_paypal_client_id_here

# MongoDB Configuration (Server-side only - no VITE_ prefix)
MONGODB_DATA_API_KEY=your_mongodb_api_key_here
MONGODB_URL=your_mongodb_url_here
MONGODB_CLUSTER=ArchLensSentinel
MONGODB_DATABASE=archlens_vault

# Environment
NODE_ENV=development
